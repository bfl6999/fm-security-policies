{
    "filename": "secret_pod1_1.yaml",
    "apiVersion": "apps_v1",
    "config": {
        "apiVersion": "apps/v1",
        "kind": "Pod",
        "metadata": {
            "name": "nginx"
        },
        "spec": {
            "containers": [
                {
                    "name": "nginx",
                    "image": "nginx:latest",
                    "env": [
                        {
                            "name": "RABBITMQ_PASSWORD",
                            "valueFrom": {
                                "secretKeyRef": {
                                    "name": "rabbitmq",
                                    "key": "RABBITMQ_PASSWORD"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}