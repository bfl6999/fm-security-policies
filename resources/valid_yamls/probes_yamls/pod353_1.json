{
    "filename": "pod353_1.yaml",
    "apiVersion": "v1",
    "config": {
        "io_k8s_api_core_v1_Pod_apiVersion": "v1",
        "io_k8s_api_core_v1_Pod_kind": "Pod",
        "io_k8s_api_core_v1_Pod_metadata": {
            "io_k8s_api_core_v1_Pod_metadata_name": "my-pod"
        },
        "io_k8s_api_core_v1_Pod_spec": {
            "io_k8s_api_core_v1_Pod_spec_containers": [
                {
                    "io_k8s_api_core_v1_Pod_spec_containers_name": "my-container",
                    "io_k8s_api_core_v1_Pod_spec_containers_image": "alpine:latest",
                    "io_k8s_api_core_v1_Pod_spec_containers_imagePullPolicy": {
                        "io_k8s_api_core_v1_Pod_spec_containers_imagePullPolicy_IfNotPresent": true
                    },
                    "io_k8s_api_core_v1_Pod_spec_containers_command": [
                        {
                            "io_k8s_api_core_v1_Pod_spec_containers_command_StringValue": "sh"
                        },
                        {
                            "io_k8s_api_core_v1_Pod_spec_containers_command_StringValue": "-c"
                        },
                        {
                            "io_k8s_api_core_v1_Pod_spec_containers_command_StringValue": "tail -f /dev/null"
                        }
                    ],
                    "io_k8s_api_core_v1_Pod_spec_containers_volumeMounts": [
                        {
                            "io_k8s_api_core_v1_Pod_spec_containers_volumeMounts_name": "secret-volume",
                            "io_k8s_api_core_v1_Pod_spec_containers_volumeMounts_mountPath": "/etc/secrets",
                            "io_k8s_api_core_v1_Pod_spec_containers_volumeMounts_readOnly": true
                        }
                    ]
                }
            ],
            "io_k8s_api_core_v1_Pod_spec_volumes": [
                {
                    "io_k8s_api_core_v1_Pod_spec_volumes_name": "secret-volume",
                    "io_k8s_api_core_v1_Pod_spec_volumes_secret": {
                        "io_k8s_api_core_v1_Pod_spec_volumes_secret_secretName": "mydatabase-secret"
                    }
                }
            ]
        }
    }
}