{
    "filename": "pods114_1.yaml",
    "apiVersion": "v1",
    "config": {
        "io_k8s_api_core_v1_Pod_apiVersion": "v1",
        "io_k8s_api_core_v1_Pod_kind": "Pod",
        "io_k8s_api_core_v1_Pod_metadata": {
            "io_k8s_api_core_v1_Pod_metadata_name": "hello-pod",
            "io_k8s_api_core_v1_Pod_metadata_labels": [
                {
                    "io_k8s_api_core_v1_Pod_metadata_labels_KeyMap": "url",
                    "io_k8s_api_core_v1_Pod_metadata_labels_ValueMap": "hello"
                }
            ]
        },
        "io_k8s_api_core_v1_Pod_spec": {
            "io_k8s_api_core_v1_Pod_spec_containers": [
                {
                    "io_k8s_api_core_v1_Pod_spec_containers_name": "hello-httpd",
                    "io_k8s_api_core_v1_Pod_spec_containers_image": "httpd:2.4-alpine",
                    "io_k8s_api_core_v1_Pod_spec_containers_command": [
                        {
                            "io_k8s_api_core_v1_Pod_spec_containers_command_StringValue": "sh"
                        },
                        {
                            "io_k8s_api_core_v1_Pod_spec_containers_command_StringValue": "-c"
                        },
                        {
                            "io_k8s_api_core_v1_Pod_spec_containers_command_StringValue": "mkdir /usr/local/apache2/htdocs/hello/; echo \"<html><body><h1>Hello, Kubernetes!</h1></body></html>\" > /usr/local/apache2/htdocs/hello/index.html; httpd -D FOREGROUND;"
                        }
                    ],
                    "io_k8s_api_core_v1_Pod_spec_containers_ports": [
                        {
                            "io_k8s_api_core_v1_Pod_spec_containers_ports_containerPort": 80
                        }
                    ]
                }
            ]
        }
    }
}