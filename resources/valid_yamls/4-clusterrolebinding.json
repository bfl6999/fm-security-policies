{
    "filename": "4-clusterrolebinding.yaml",
    "apiVersion": "v1",
    "policies": {
        "Disallow_Host_Namespaces": true,
        "Restrict_AppArmor" : true,
        "Disallow_hostPath" : true,
        "Disallow_Privileged_Containers": true,
        "Restrict_Auto_Mount_of_Service_Account_Tokens_in_Service_Account": true,
        "Restrict_Binding_to_Cluster_Admin": true,
        "Restrict_Binding_System_Groups": true
    },
    "config": {
        "io_k8s_api_rbac_v1_ClusterRoleBinding_kind": "ClusterRoleBinding",
        "io_k8s_api_rbac_v1_ClusterRoleBinding_apiVersion": "rbac.authorization.k8s.io/v1",
        "io_k8s_api_rbac_v1_ClusterRoleBinding_metadata": {
