{
    "filename": "01-Pod-Annotation.yaml",
    "apiVersion": "v1",
    "policies": {
        "Disallow_Host_Namespaces": true,
        "Restrict_AppArmor" : true
    },
    "config": {
        "io_k8s_api_core_v1_Pod_apiVersion": "v1",
        "io_k8s_api_core_v1_Pod_kind": "Pod",
        "io_k8s_api_core_v1_Pod_metadata": {
            "io_k8s_api_core_v1_Pod_metadata_name": "annotated-pod",
            "io_k8s_api_core_v1_Pod_metadata_annotations": [
                {
                    "io_k8s_api_core_v1_Pod_metadata_annotations_KeyMap": "container_apparmor_security_beta_kubernetes_io",
                    "io_k8s_api_core_v1_Pod_metadata_annotations_ValueMap": "insecure-profile"
                },
                {
                    "io_k8s_api_core_v1_Pod_metadata_annotations_KeyMap": "owner",
                    "io_k8s_api_core_v1_Pod_metadata_annotations_ValueMap": "dev-team"
                }
            ]
        },
        "io_k8s_api_core_v1_Pod_spec": {
            "io_k8s_api_core_v1_Pod_spec_imagePullSecrets": [
                {
                    "io_k8s_api_core_v1_Pod_spec_imagePullSecrets_name": "regcred"
                }
            ],
            "io_k8s_api_core_v1_Pod_spec_containers": [
                {
                    "io_k8s_api_core_v1_Pod_spec_containers_name": "nginx",
                    "io_k8s_api_core_v1_Pod_spec_containers_image": "nginx:latest"
                }
            ]
        }
    }
}